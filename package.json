{
  "name": "Transactly",
  "version": "1.0.0",
  "description": "An Account Abstraction Smart Wallet",
  "main": "server.js",
  "scripts": {
    "tsc": "tsc",
    "build": "tsc",
    "dev:user": "ts-node-dev source/servers/user.server.ts",
    "runBuild": "pm2 start ecosystem.yml --only node-app",
    "dev:admin": "ts-node-dev source/servers/admin.server.ts",
    "dev:all": "start npm run dev:user && start npm run dev:admin",
    "migrate": "prisma migrate dev --name",
    "prisma:migrate": "npx prisma migrate dev --skip-seed",
    "prisma:generate": "npx prisma generate",
    "prisma:format": "npx prisma format",
    "prisma:push": "npx prisma db push",
    "clean": "rm -rf ./build",
    "test": "NODE_ENV=test && jest --testTimeout=10000 --forceExit --coverage --verbose --runInBand"
  },
  "keywords": [
    "wallet",
    "account-abstraction"
  ],
  "author": "Okeke Francis",
  "license": "ISC",
  "dependencies": {
    "@prisma/adapter-pg": "^5.16.1",
    "@prisma/client": "^5.16.1",
    "aws-sdk": "^2.1272.0",
    "axios": "^1.2.1",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.18.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "ethers": "^5.4.0",
    "express": "^4.19.2",
    "express-fileupload": "^1.4.0",
    "express-handlebars": "^7.0.7",
    "express-session": "^1.18.0",
    "getmac": "^5.20.0",
    "handlebars": "^4.7.7",
    "ioredis": "^5.3.0",
    "joi": "^17.13.3",
    "joi-password-complexity": "^5.1.0",
    "json": "^11.0.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^5.0.7",
    "nodemailer": "^6.9.14",
    "pg": "^8.12.0",
    "sinon": "^15.0.4",
    "string-similarity-js": "^2.1.4",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/express-fileupload": "^1.4.1",
    "@types/fingerprintjs2": "^2.0.0",
    "@types/jest": "^29.5.12",
    "@types/joi": "^17.2.3",
    "@types/json2csv": "^5.0.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.13.0",
    "@types/pg": "^8.11.6",
    "@types/sinon": "^10.0.14",
    "@types/sinon-chai": "^3.2.9",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.7.0",
    "prettier": "3.0.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.3"
